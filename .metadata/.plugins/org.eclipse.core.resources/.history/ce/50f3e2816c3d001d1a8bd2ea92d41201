<%@ page language="java" contentType="text/html; charset=UTF-8"%>
<!DOCTYPE html>
<html>
<head>
<title>Scripting Tag</title>
</head>
<body>
	<%! // 선언문 태그
		// 변수 data에 50을 할당(저장)
		int data = 50;
	%>
	<%
		// out : JSP 내장 객체(그냥 주어짐).
		out.println("Value of the variable is : " + data);
	%>
</body>
</html>